---
pagination:
  data: collections.docs
  size: 1
  alias: page
permalink: /{{ page.slug }}/
---
{% layout "layouts/base" %}

{% block header %}{% include 'partials/header',title:page.title %}{% endblock %}

{% block head %}{% include 'partials/head',title:page.title %}{% endblock %}


{% block content %}
  <article class="markdown book-article">
    <h1 class="book-title">
     {{ page.title }}
    </h1>
    {{ page.html }}
  </article>
  {% include "partials/post-action",post:page %}

{% endblock %}


{% block footer %}
{% include 'partials/footer',post:page %}
{% endblock %}

{% block comments %}
{% include 'partials/comment' %}
{% endblock %}



{% block toc %}
{% include 'partials/toc',post:page %}
{% endblock %}


{% block h-toc %}
<aside class="hidden clearfix">
{% include 'partials/toc',post:page %}
</aside>
{% endblock %}



{% block pagination %}{% endblock %}

