---
permalink: /map/index.html
---
{% for doc in collections.docs-all %}
{% if doc.slug == 'map' %}
{% assign page = doc %}
{% break %}
{% endif %}
{% endfor %}

{% layout "layouts/base" %}


{% block head %}{% include 'partials/head',title:post.title %}{% endblock %}

{% block header %}{% include 'partials/header',title:post.title %}{% endblock %}


{% block content %}
<article class="markdown book-article">
  <h1 class="book-title">
    {{ page.title }}
  </h1>

  {{ page.html }}
  <script type="module" src="{{ '/assets/map.js' | absoluteUrl | cacheBust }}"></script>

  <div id="map" class="footer-map"></div>

  <script type="module" defer >
    import initMap from '/assets/map.js';
    initMap();
  </script>


</article>
{% include "partials/post-action",post:page %}
{% endblock %}


{% block footer %}
{% include 'partials/footer',post:page %}
{% endblock %}

{% block comments %}
{% include 'partials/comment' %}
{% endblock %}


{% block toc %}
{% comment %} {% include 'partials/toc-tagloop' %} {% endcomment %}
{% endblock %}

{% block h-toc %}
{% comment %}
<aside class="hidden clearfix">
  {% include 'partials/toc-tagloop' %}
</aside>
{% endcomment %}
{% endblock %}